{
    "name": "chrisnoden/synergy",
    "type": "library",
    "description": "Synergy MVC framework for rapid web development",
    "keywords": ["mvc", "framework", "symfony"],
    "homepage": "https://github.com/chrisnoden/synergy",
    "license": "Apache-2.0",
    "version": "0.1.0-dev",
    "minimum-stability": "dev",
    "authors": [
        { "name": "Chris Noden", "email": "chris.noden@gmail.com", "homepage": "http://www.chrisnoden.com" }
    ],
    "require": {
        "php": ">=5.3.21",
        "ext-Core": "*",
        "ext-Reflection": "*",
        "ext-curl": "*",
        "ext-SPL": "*",
        "ext-date": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-posix": "*",
        "ext-standard": "*",
        "psr/log": "1.0.0",
        "symfony/http-foundation": "2.2.*",
        "symfony/routing": "2.2.*",
        "symfony/config": "2.2.*",
        "symfony/yaml": "2.2.*",
        "wp-cli/php-cli-tools": "dev-master"
    },
    "suggest": {
        "chrisnoden/talkback": "1.*",
        "mobiledetect/mobiledetectlib": "2.6.*"
    },
    "autoload": {
        "psr-0": { "Synergy": "src/" },
        "classmap": [ "src/UASparser" ]
    }
}