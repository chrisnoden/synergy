{
    "name": "chrisnoden/synergy",
    "type": "library",
    "description": "Synergy MVC framework for rapid web development",
    "keywords": ["mvc", "framework", "symfony"],
    "homepage": "https://github.com/chrisnoden/synergy",
    "license": "Apache-2.0",
    "version": "0.1.0-dev",
    "minimum-stability": "beta",
    "authors": [
        { "name": "Chris Noden", "email": "chris.noden@gmail.com", "homepage": "http://www.chrisnoden.com" }
    ],
    "require": {
        "php": ">=5.3.21",
        "ext-reflection": "*",
        "ext-curl": "*",
        "ext-spl": "*",
        "ext-date": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-posix": "*",
        "psr/log": "1.0.0",
        "mustangostang/spyc": "0.5.1",
        "symfony/http-foundation": "2.2.*",
        "symfony/routing": "2.2.*",
        "symfony/config": "2.2.*",
        "symfony/yaml": "2.2.*",
        "wp-cli/php-cli-tools": "0.9.*"
    },
    "require-dev": {
        "chrisnoden/talkback": "1.*",
        "mobiledetect/mobiledetectlib": "2.6.*",
        "smarty/smarty": "3.1.*",
        "propel/propel1": "1.6.*",
        "twig/twig": "v1.13.1"
    },
    "suggest": {
        "chrisnoden/talkback": ">=1.0.1",
        "mobiledetect/mobiledetectlib": "2.6.*"
    },
    "autoload": {
        "psr-0": { "Synergy": "src/" },
        "classmap": [ "src/UASparser" ]
    },
    "bin": [ "bin/console","bin/daemon" ]

}