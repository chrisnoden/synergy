{
    "name": "chrisnoden/synergy",
    "type": "library",
    "description": "Synergy MVC framework for rapid web development",
    "keywords": ["mvc", "framework", "symfony"],
    "homepage": "https://github.com/chrisnoden/synergy",
    "license": "Apache-2.0",
    "version": "0.3.0-dev",
    "minimum-stability": "dev",
    "authors": [
        { "name": "Chris Noden", "email": "chris.noden@gmail.com", "homepage": "http://www.chrisnoden.com" }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/chrisnoden/Propel"
        },
        {
            "type": "vcs",
            "url": "https://github.com/chrisnoden/php-cli-tools"
        }
    ],
    "require": {
        "php": ">=5.4.13",
        "ext-reflection": "*",
        "ext-curl": "*",
        "ext-spl": "*",
        "ext-date": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-posix": "*",
        "psr/log": "1.0.0",
        "mustangostang/spyc": "0.5.1",
        "symfony/http-foundation": "2.2.*",
        "symfony/routing": "2.2.*",
        "symfony/config": "2.2.*",
        "symfony/yaml": "2.2.*",
        "eloquent/enumeration": "5.0.*"
    },
    "require-dev": {
        "chrisnoden/talkback": "dev-master",
        "mobiledetect/mobiledetectlib": "dev-master",
        "smarty/smarty": "3.1.*",
        "propel/propel1": "dev-master",
        "jlogsdon/cli": "dev-master",
        "twig/twig": "1.13.*"
    },
    "suggest": {
        "chrisnoden/talkback": ">=1.0.1",
        "mobiledetect/mobiledetectlib": "2.6.*"
    },
    "autoload": {
        "psr-0": { "Synergy": "src/" }
    },
    "bin": [ "bin/console","bin/daemon" ]

}